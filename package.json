{
  "name": "moss-monitor",
  "version": "0.0.3",
  "description": "前端监控本地数据采集SDK",
  "author": "craber-dev",
  "license": "ISC",
  "main": "./dist/cjs/main.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --outDir dist/cjs",
    "prepare": "husky install"
  },
  "keywords": [
    "monitor"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.5",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@types/uuid": "^9.0.1",
    "axios": "^1.3.4",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0"
  }
}
